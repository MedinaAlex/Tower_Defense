package com.epsi.tower_defense;
import java.util.ArrayList;

public class Case {
    boolean chemin = false;
    ArrayList<Ennemi> listEnnemis = new ArrayList<>();
    Tour tour = null;
    boolean depart = false;
    boolean arrive = false;
    int x;
    int y;

    public Case (int x, int y, boolean chemin, boolean depart, boolean arrive){
    	this.x = x;
    	this.y = y;
        this.chemin = chemin;
        this.depart = depart;
        this.arrive = arrive;
    }

	@Override
	public String toString() {
		return "Case [chemin=" + chemin + ", listEnnemis=" + listEnnemis + ", tour=" + tour + ", depart=" + depart
				+ ", arrive=" + arrive + "]";
	}   
}
