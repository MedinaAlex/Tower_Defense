package com.epsi.tower_defense;

import java.io.IOException;
import java.util.ArrayList;

public class App {
	public static void main(String[] args) throws IOException {
		Plateau plateau = new Plateau("ressources/terrainTest.json");
		ArrayList<Case> chemin = plateau.getChemin();
		System.out.println("size = " + chemin.size());
		for(Case c: chemin){
			System.out.println(c);
		}
		Ennemi ennemi = new Ennemi("bob", 1, 10, 1, 1);
		chemin.get(0).listEnnemis.add(ennemi);
	}
}
