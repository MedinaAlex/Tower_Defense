package com.epsi.tower_defense;

import java.io.IOException;


public class App {
	public static void main(String[] args) throws IOException {
		Plateau plateau = new Plateau("ressources/terrainTest.json");
		
		Ennemi ennemi = new Ennemi("bob", 1, 35, 50, 1);
		Ennemi ennemi2 = new Ennemi("bob2", 1, 35, 50, 1);
		plateau.chemin.get(0).listEnnemis.add(ennemi);
		plateau.chemin.get(0).listEnnemis.add(ennemi2);
		
		plateau.run();
	}
}
